<% form_for [:admin, @service_category] do |f| -%>
  
  <%= f.error_messages %>

  <fieldset>
    <%= f.label :name, t("service_category.name") %>
    <%= f.text_field :name %> <br />
    <%= f.label :proficiency, t("service_category.proficiency") %>
    <%= f.check_box :proficiency %><br />
  </fieldset>

  <%= f.submit t('forms.save') %> <%= t('or') %> <%= link_to t('cancel'), admin_service_categories_path %>
<% end -%>
